          <thumbopacity>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.thumbopacity</label>
              <config>
                <type>input</type>
                <size>3</size>
                <eval>double,trim</eval>
                <checkbox>0</checkbox>
              </config>
            </TCEforms>
          </thumbopacity>

          <thumbspacing>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.thumbspacing</label>
              <config>
                <type>input</type>
                <size>4</size>
                <eval>trim</eval>
                <checkbox></checkbox>
              </config>
            </TCEforms>
          </thumbspacing>

          <infopane>
            <TCEforms>
            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.infopane</label>
            <config>
              <type>check</type>
            </config>
            </TCEforms>
          </infopane>

          <slideinfozoneopacity>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.slideinfozoneopacity</label>
              <config>
                <type>input</type>
                <size>3</size>
                <eval>double,trim</eval>
                <checkbox>0</checkbox>
              </config>
            </TCEforms>
          </slideinfozoneopacity>

          <carousel>
            <TCEforms>
            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.carousel</label>
            <config>
              <type>check</type>
            </config>
            </TCEforms>
          </carousel>

          <externalthumbs>
              <TCEforms>
                  <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.externalthumbs</label>
                  <config>
                      <type>input</type>
                      <size>10</size>
                      <checkbox></checkbox>
                  </config>
              </TCEforms>
          </externalthumbs>

          <externalcontrol>
            <TCEforms>
            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.externalcontrol</label>
            <config>
              <type>check</type>
            </config>
            </TCEforms>
          </externalcontrol>

           <advancedsettings>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.advancedsettings</label>
              <config>
                <type>text</type>
                <cols>30</cols>
                <rows>2</rows>
              </config>
            </TCEforms>
          </advancedsettings>


        </el>
      </ROOT>
    </advanced>

  </sheets>
</T3DataStructure>
