<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                <sheetTitle>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.title</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                     <mode>
                        <TCEforms>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.0</numIndex>
                                        <numIndex index="1">DIRECTORY</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.1</numIndex>
                                        <numIndex index="1">RECORDS</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.3</numIndex>
                                        <numIndex index="1">DAM</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.4</numIndex>
                                        <numIndex index="1">DAMCAT</numIndex>
                                    </numIndex>
                </items>
                            </config>
                        </TCEforms>
                    </mode>

                    <time>
                        <TCEforms>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.time</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                                <!-- <eval>int</eval> -->
                <range>
                  <lower>1000</lower>
                  <upper>30000</upper>
                </range>
                            </config>
                        </TCEforms>
                    </time>

                  <startingpoint>
                      <TCEforms>
                      <displayCond>FIELD:mode:!IN:RECORDS,DAM</displayCond>
                          <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.startingpoint</label>
                          <config>
                              <type>input</type>
                              <size>25</size>
                              <eval>trim</eval>
                              <wizards>
                  <_PADDING>2</_PADDING>
                  <link>
                    <type>popup</type>
                    <title>Folder Selector</title>
                    <icon>link_popup.gif</icon>
                    <script>EXT:rgfolderselector/browse_links.php</script>
                    <JSopenParams>height=400,width=400,status=0,menubar=0,scrollbars=1</JSopenParams>
                  </link>
                              </wizards>
                          </config>
                      </TCEforms>
                  </startingpoint>

          <text>
            <TCEforms>
              <displayCond>FIELD:mode:!IN:RECORDS,DAM,DAMCAT</displayCond>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.text</label>
              <config>
                <type>text</type>
                <cols>40</cols>
                <rows>10</rows>
              </config>
            </TCEforms>
          </text>

                    <startingpointrecords>
                        <TCEforms>
                        <displayCond>FIELD:mode:=:RECORDS</displayCond>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.startingpointrecords</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>50</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <autoSizeMax>30</autoSizeMax>
                            </config>
                        </TCEforms>
                    </startingpointrecords>

                    <startingpointdam>
                        <TCEforms>
                        <displayCond>FIELD:mode:=:DAM</displayCond>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.startingpointdam</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_dam</allowed>
                                <size>10</size>
                                <prepend_tname>1</prepend_tname>
                                <MM>tx_dam_mm_ref</MM>
                                <MM_foreign_select>1</MM_foreign_select>
                                <MM_opposite_field>1</MM_opposite_field>
                                <MM_match_fields>
                                  <ident>mojo_sleekgallery</ident>
                                </MM_match_fields>
                                <disallowed_types>php,php3</disallowed_types>
                                <maxitems>50</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <autoSizeMax>30</autoSizeMax>
                            </config>
                        </TCEforms>
                    </startingpointdam>

                    <startingpointdamcat>
                        <TCEforms>
                        <displayCond>FIELD:mode:=:DAMCAT</displayCond>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.startingpointdamcat</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_dam_cat</allowed>
                                <size>5</size>
                                <prepend_tname>1</prepend_tname>
                                <disallowed_types>php,php3</disallowed_types>
                                <maxitems>50</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <autoSizeMax>30</autoSizeMax>
                            </config>
                        </TCEforms>
                    </startingpointdamcat>

              <recursivedamcat>
                  <TCEforms>
                        <displayCond>FIELD:mode:=:DAMCAT</displayCond>
                  <label>LLL:EXT:lang/locallang_general.php:LGL.recursive</label>
                      <config>
                          <type>select</type>
                          <items type="array">
                              <numIndex index="0" type="array">
                                  <numIndex index="0"></numIndex>
                                  <numIndex index="0"></numIndex>
                              </numIndex>
                              <numIndex index="1" type="array">
                                  <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.1</numIndex>
                                  <numIndex index="1">1</numIndex>
                              </numIndex>
                              <numIndex index="2" type="array">
                                  <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.2</numIndex>
                                  <numIndex index="1">2</numIndex>
                              </numIndex>
                              <numIndex index="3" type="array">
                                  <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.3</numIndex>
                                  <numIndex index="1">3</numIndex>
                              </numIndex>
                              <numIndex index="4" type="array">
                                  <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.4</numIndex>
                                  <numIndex index="1">4</numIndex>
                              </numIndex>
                              <numIndex index="5" type="array">
                                  <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.5</numIndex>
                                  <numIndex index="1">250</numIndex>
                              </numIndex>
                          </items>
                          <minitems>0</minitems>
                          <maxitems>1</maxitems>
                          <size>1</size>
                      </config>
                  </TCEforms>
              </recursivedamcat>

                    <width>
                        <TCEforms>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.width</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                            </config>
                        </TCEforms>
                    </width>

                    <height>
                        <TCEforms>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.height</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                            </config>
                        </TCEforms>
                    </height>

                    <widthgallery>
                        <TCEforms>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.widthgallery</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                            </config>
                        </TCEforms>
                    </widthgallery>

                    <heightgallery>
                        <TCEforms>
                            <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.general.heightgallery</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                            </config>
                        </TCEforms>
                    </heightgallery>

        </el>
      </ROOT>
    </sDEF>

        <advanced>
            <ROOT>
                <TCEforms>
                <sheetTitle>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.title</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

          <lightbox>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.lightbox</label>
              <config>
                <type>check</type>
                <default>1</default>
              </config>
            </TCEforms>
          </lightbox>



          <arrows>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.arrows</label>
              <config>
                <type>check</type>
                <default>1</default>
              </config>
            </TCEforms>
          </arrows>

          <showThumbs>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.showThumbs</label>
              <config>
                <type>check</type>
                <default>1</default>
              </config>
            </TCEforms>
          </showThumbs>

          <showPlay>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.showPlay</label>
              <config>
                <type>check</type>
                <default>0</default>
              </config>
            </TCEforms>
          </showPlay>

          <watermark>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.watermark</label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </watermark>

          <limitImagesDisplayed>
            <TCEforms>
              <label>LLL:EXT:mojo_sleekgallery/Resources/Private/Language/locallang.xml:flexform.sheets.advanced.limitImagesDisplayed</label>
              <config>
                        <type>input</type>
                        <size>3</size>
                        <eval>int,trim</eval>
                <checkbox>0</checkbox>
              </config>
            </TCEforms>
          </limitImagesDisplayed>



